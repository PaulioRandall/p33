{
	"author": "Paul Williams",
	"name": "p33",
	"description": "A simple library for generating coordinates of Pythagorean Triangles that are intended for visualisation. Made to be plundered.",
	"type": "module",
	"license": "MIT",
	"version": "0.5.0",
	"keywords": [
		"visualisation",
		"geometry",
		"svg",
		"pythagorean-triples"
	],
	"exports": {
		".": "./src/index.js"
	},
	"engines": {
		"node": ">=18.18.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/PaulioRandall/p33.git"
	},
	"scripts": {
		"fmt": "prettier --log-level warn --write .",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --silent --runInBand",
		"debug": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
		"commit": "npm run fmt && npm run test && cat ./scripts/ready-to-ship.txt"
	},
	"devDependencies": {
		"jest": "^29.7.0",
		"prettier": "^3.1.0"
	},
	"dependencies": {
		"victor": "^1.1.0"
	}
}
